#include <iostream>
using namespace std;
int main()
{
	int A[3][4];
	//1.satır
	A[0][0] = 3;
	A[0][1] = 4;
	A[0][2] = 56;
	A[0][3] = 4;
	//2.satır
	A[1][0] = 23;
	A[1][1] = 34;
	A[1][2] = 56;
	A[1][3] = 45;
	//3.satır
	A[2][0] = 3;
	A[2][1] = 67;
	A[2][2] = 76;
	A[2][3] = 50;
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 4; j++)
			cout << A[i][j] << "\t";
		cout << endl;
	}
	
	system("pause");
	return 0;
}
