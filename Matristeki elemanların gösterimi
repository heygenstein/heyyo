#include<iostream> 
using namespace std; 
int main()
{
	int deneme[3][2] = { {1,2},{5,7},{8,4} };
	int x, y;
	cout << "Bu dizinin elemanlari" << endl;
	for (x = 0; x < 3;x++)
	{
		for (y = 0; y < 2; y++)
		{
			cout << "[" << x << "][" << y << "]:";
			cout << deneme[x][y] << endl;
		}
	}
	system("pause");
	return 0;
}
