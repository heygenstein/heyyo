//dizi[satır][sütun]
#include <iostream>
using namespace std;
int main()
{
	int matris[3][3];
	for (int sat = 0; sat < 3; sat++)
	{
		for (int sut = 0; sut < 3; sut++)
		{
			cout << sat + 1 << ". Satir ve " << sut + 1 << ". Sutun Degerini Giriniz:";
			cin >> matris[sat][sut];
		}
	}
	for (int i = 0; i < 3; i++)
	{
		for (int k = 0; k < 3; k++)
		{
			cout << matris[i][k] << "\t";
		}
		cout << endl;
	}
	return 0;
}
